generator client {
  provider      = "prisma-client-js"
  engineType    = "library"
}

generator zod {
  provider       = "zod-prisma-types"
  output         = "../../src/zod"
  createInputTypes = true
  createModelTypes = true
  createRelationValuesTypes = true
  addInputTypeValidation = false
  useDefaultValidators = false
  coerceDate = true
}

datasource db {
  provider = "postgresql"
}
